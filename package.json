{
  "name": "styled-scopify",
  "version": "0.0.5",
  "author": "Wes Moberly <github.wes@mailhero.io>",
  "license": "MIT",
  "description": "A wrapper function for styled-components that scopes all of your CSS rules to a root selector",
  "main": "lib/index.js",
  "files": "lib/**/*.js",
  "repository": "https://github.com/wesm87/styled-scopify.git",
  "scripts": {
    "build": "rm -rf lib/* && babel src --out-dir lib --no-comments",
    "prepublishOnly": "yarn build",
    "postversion": "git push && git push --tags",
    "lint": "eslint --ext .js src"
  },
  "dependencies": {
    "ramda": "^0.25.0",
    "styled-transform-proxy": "^0.0.7"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-plugin-transform-imports": "^1.5.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "confusing-browser-globals": "^2.0.0-next.9754a231",
    "eslint": "^4.19.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.9.0",
    "flow-bin": "^0.68.0",
    "flow-typed": "^2.4.0"
  }
}
